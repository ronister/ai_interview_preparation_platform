    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django Server Debug",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver"],
            "django": true,
            "console": "integratedTerminal",
            "env": {
                "IS_EXECUTION_ENVIRONMENT": "false"
            },
            "preLaunchTask": "yarn-start"
        },
        {
            "name": "test_db_connection",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test/test_db_connection.py",
            "console": "integratedTerminal"
        },
        {
            "name": "test_sample_data",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test/test_sample_data.py",
            "console": "integratedTerminal"
        },
        {
            "name": "instructions_difficulty_eval",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/apps/instructions_difficulty_eval/instructions_difficulty_eval.py",
            "console": "integratedTerminal"
        },
        {
            "name": "codebase_to_text_convert",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/codebase_to_text_convert/codebase_to_text_convert.py",
            "console": "integratedTerminal"
        },
        {
            "name": "test_grading_changes",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tests/test_grading_changes.py",
            "console": "integratedTerminal"
        }
    ]
}